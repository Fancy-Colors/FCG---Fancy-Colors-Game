.navigation {
  width: 322px;
  height: 48px;
  background-color: transparent;
  padding: 12px;

  & [data-color='stroke-color'] path {
    stroke: var(--color-dark-primary, #0f101b);
  }

  & [data-color='fill-color'] path {
    stroke: var(--color-dark-primary, #0f101b);
  }
}

.active {
  background-color: var(--color-light-primary, #f5f8fa);
}
