.section {
  display: flex;
  flex-flow: nowrap;
  gap: 24px;
  max-height: 100vh;
  overflow: hidden;
}

.content {
  width: 100%;
  max-height: 100vh;
  padding: 24px;
  display: flex;
  flex-flow: column;
  gap: 24px;
  overflow: hidden;
  background-color: var(--color-light-primary-opacity, #f5f8fa88);
}
