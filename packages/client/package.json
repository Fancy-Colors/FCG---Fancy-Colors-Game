{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/server/assets/js/entry-server.js",
  "exports": {
    ".": {
      "import": "./dist/server/assets/js/entry-server.js"
    },
    "./index.html": "./dist/client/index.html",
    "./dev/index.html": "./index.html",
    "./dev/entry-server": "./src/entry-server.tsx",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir dist/client",
    "build:ssr": "vite build --outDir dist/server --ssr ./src/entry-server.tsx",
    "preview": "vite preview --host",
    "prepare": "run-p build build:ssr",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --cache --cache-location ./node_modules/.cache/eslint",
    "lint:tsc": "tsc --noEmit",
    "lint:stylelint": "stylelint \"src/**/*.{css,pcss}\" --cache --cache-location ./node_modules/.cache/stylelint",
    "format": "run-s format:*",
    "format:prettier": "prettier --write --loglevel \"warn\" \"**/*.{js,ts,tsx,json,css,pcss,html,md}\"",
    "format:eslint": "eslint --fix --ext .ts,.tsx",
    "format:stylelint": "stylelint --fix \"src/**/*.{css,pcss}\"",
    "test": "jest"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.0.0-alpha.1",
    "classnames": "2.3.2",
    "colorjs.io": "0.4.3",
    "dotenv": "16.0.2",
    "js-cookie": "3.0.1",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.2",
    "react-redux": "8.0.5",
    "react-router-dom": "6.10.0",
    "swiper": "9.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.4.0",
    "@types/js-cookie": "3.0.3",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "eslint": "8.34.0",
    "eslint-config-shared": "*",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.10.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "npm-run-all": "4.1.5",
    "postcss-nested": "6.0.1",
    "prettier": "2.8.4",
    "stylelint": "15.2.0",
    "stylelint-config-standard": "30.0.1",
    "stylelint-csstree-validator": "2.1.0",
    "stylelint-declaration-block-no-ignored-properties": "2.7.0",
    "ts-jest": "29.0.5",
    "ts-jest-mock-import-meta": "1.0.0",
    "typescript": "4.9.5",
    "vite": "4.2.1",
    "vite-plugin-svgr": "2.4.0",
    "whatwg-fetch": "3.6.2"
  },
  "license": "MIT"
}
