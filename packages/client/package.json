{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --cache --cache-location ./node_modules/.cache/eslint",
    "lint:tsc": "tsc --noEmit",
    "lint:stylelint": "stylelint \"src/**/*.{css,pcss}\" --cache --cache-location ./node_modules/.cache/stylelint",
    "format": "run-s format:*",
    "format:prettier": "prettier --write --loglevel \"warn\" \"**/*.{js,ts,tsx,json,css,pcss,html,md}\"",
    "format:eslint": "eslint --fix --ext .ts,.tsx",
    "format:stylelint": "stylelint --fix \"src/**/*.{css,pcss}\"",
    "test": "jest"
  },
  "dependencies": {
    "classnames": "2.3.2",
    "dotenv": "^16.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.43.2",
    "react-router-dom": "6.8.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-shared": "*",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28",
    "jest-environment-jsdom": "^29.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-nested": "^6.0.1",
    "prettier": "^2.7.1",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-csstree-validator": "^2.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "vite": "^3.0.7",
    "vite-plugin-svgr": "^2.4.0"
  },
  "license": "MIT"
}
